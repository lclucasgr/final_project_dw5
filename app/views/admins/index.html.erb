<div class="center jumbotron">
  <h3>Livros Cadastrados</h3>

  <%= render 'layouts/header_admin' %>

  <table class="table">
  <thead>
    <tr>
      <th scope="col">Nome</th>
      <th scope="col">Email</th>
    </tr>
  </thead>

<% @admins.each do |admin| %>
  <tr>
    <th scope="col"><%= admin.name %></th>
    <th scope="col"><%= admin.email %></th>
  </tr>
<% end %>
</table>

<br>
